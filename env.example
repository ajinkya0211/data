# Backend Configuration
DATABASE_URL=postgresql://notebook_user:notebook_pass@localhost:5432/notebooks
REDIS_URL=redis://localhost:6379
MINIO_URL=http://localhost:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin123

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# AI Provider Configuration
# Default provider (ollama, openai, gemini)
DEFAULT_AI_PROVIDER=ollama

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4.1-mini
OPENAI_MAX_TOKENS=4000
OPENAI_TEMPERATURE=0.7
OPENAI_BASE_URL=https://api.openai.com/v1

# Google Gemini Configuration
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-1.5-pro
GEMINI_MAX_TOKENS=4000
GEMINI_TEMPERATURE=0.7

# Ollama Configuration (Local)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_DEFAULT_MODEL=llama3.2:3b
OLLAMA_TIMEOUT=120
OLLAMA_MAX_TOKENS=4000
OLLAMA_TEMPERATURE=0.7

# Jupyter Kernel Configuration
JUPYTER_KERNEL_URL=http://localhost:8888
JUPYTER_KERNEL_TOKEN=notebook_token_123
KERNEL_TIMEOUT_SECONDS=300

# File Upload Configuration
MAX_FILE_SIZE=104857600
ALLOWED_FILE_TYPES=.csv,.parquet,.json,.xlsx,.ipynb

# Execution Configuration
MAX_CONCURRENT_RUNS=10
MAX_BLOCKS_PER_RUN=100

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Development Configuration
DEBUG=true
ALLOWED_HOSTS=["*"]
CORS_ORIGINS=["http://localhost:3000", "http://127.0.0.1:3000"]

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_ENVIRONMENT=development 